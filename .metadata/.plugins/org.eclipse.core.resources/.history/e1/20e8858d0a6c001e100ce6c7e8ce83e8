package CollectionGenerics;

import java.util.*;

public class MapDemo {
	
	public static void main(String [] args) {
		Map<Integer,String> hs = new HashMap<>();
		hs.put(1,"Hey");
		hs.put(2, "Hello");
		hs.put(3, "Hola");
		hs.put(4, "Hii");
		
		for(Map.Entry<Integer,String> m : hs.entrySet()) {
			System.out.println(m.getKey() + " " + m.getValue());
		}
	}

}
